<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<AbilityDef>
  <defName>JJK_IdleTransfigurationRevertDoll</defName>
  <label>Idle Transfiguration : Revert Doll</label>
  <description>Transfigure the target doll back into a pawn.</description>
  <iconPath>UI/Abilities/ShapeFlesh</iconPath>
  <category>Sanguophage</category>
  <displayOrder>100</displayOrder>
  <warmupStartSound>Bloodfeed_Cast</warmupStartSound>
  <displayGizmoWhileUndrafted>True</displayGizmoWhileUndrafted>
  <disableGizmoWhileUndrafted>False</disableGizmoWhileUndrafted>
  <hostile>true</hostile>
  <verbProperties>
    <verbClass>Verb_CastAbility</verbClass>
    <drawAimPie>false</drawAimPie>
    <range>5</range>
    <warmupTime>0.5</warmupTime>
    <targetParams>
      <canTargetPawns>False</canTargetPawns>
      <canTargetItems>False</canTargetItems>
      <canTargetLocations>True</canTargetLocations>
    </targetParams>
  </verbProperties>
  <comps>
    <li Class="JJK.CompProperties_IdleTransfigurationRespawn">
        <compClass>JJK.CompAbilityEffect_IdleTransfigurationRespawn</compClass>
    </li>
    <li Class="JJK.CompProperties_AbilityCECost">
          <cursedEnergyCost>5</cursedEnergyCost>
    </li>
  </comps>
</AbilityDef>
</Defs>
