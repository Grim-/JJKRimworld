<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<HediffDef>
  <defName>JJK_PlayfulCloudConcussion</defName>
  <label>Concussed</label>
  <hediffClass>HediffWithComps</hediffClass>
  <description>Hit by PlayfulCloud partially stunning, reducing movement by 20%.</description>
  <tendable>false</tendable>
  <initialSeverity>1</initialSeverity>
  <stages>
    <li>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <postFactor>0.2</postFactor>
          </li>
        </capMods>
    </li>
  </stages>
  <comps>
    <li Class="HediffCompProperties_Disappears">
      <disappearsAfterTicks>1250</disappearsAfterTicks><!-- 6 hours -->
    </li>
  </comps>
</HediffDef>
</Defs>
