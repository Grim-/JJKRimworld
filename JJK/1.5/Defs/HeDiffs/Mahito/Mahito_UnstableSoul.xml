    <li Class="HediffCompProperties_DisappearsAndKills">
      <disappearsAfterTicks>420000</disappearsAfterTicks><!-- one week -->
      <showRemainingTime>true</showRemainingTime>
    </li>
  </comps>
  <stages>
    <li>
      <statFactors>
        <InjuryHealingFactor>0</InjuryHealingFactor>
      </statFactors>
    </li>
  </stages>
</HediffDef>
</Defs>
